{
  "name": "reskyt-pixel",
  "vendor": "vtexspain",
  "version": "0.0.1",
  "title": "Reskyt Pixel",
  "description": "Reskyt Pixel creates a banner with a link to the app store",
  "billingOptions": {
    "termsURL": "https://compliance.vtex.com/gdpr/policies/vtex-privacy-policy",
    "support": {
      "url": "https://support.vtex.com/hc/requests"
    },
    "free": true,
    "type": "free",
    "availableCountries": ["*"]
  },
  "builders": {
    "react": "3.x",
    "store": "0.x",
    "pixel": "0.x",
    "docs": "0.x"
  },
  "dependencies": {
    "vtex.pixel-interfaces": "1.x"
  },
  "settingsSchema": {
    "title": "Reskyt Pixel",
    "type": "object",
    "properties": {
      "appName": {
        "title": "App Name",
        "description": "App name",
        "type": "string"
      },
      "fabName": {
        "title": "Developer name",
        "description": "Developer name",
        "type": "string"
      },
      "htmlTitle": {
        "title": "HTML Title Name",
        "description": "The HTML title for the pixel",
        "type": "string"
      },
      "downloadLink": {
        "title": "App download link",
        "description": "Link to app store",
        "type": "string"
      },
      "appImage": {
        "title": "App image",
        "description": "Store app image",
        "type": "string"
      },
      "appImageAlt": {
        "title": "Image alt",
        "description": "Image alternative text",
        "type": "string"
      }
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
